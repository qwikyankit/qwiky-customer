# Environment
NODE_ENV=development

# Backend API URL (point this to Render deployment in production)
VITE_API_BASE_URL=https://qwiky-backend.onrender.com

# Payment Configuration - Cashfree
VITE_PAYMENT_MODE=cashfree-test
# Options: cashfree-test, cashfree-live
VITE_CASHFREE_ENV=TEST
# Options: TEST, PRODUCTION

# Google Maps
VITE_GOOGLE_MAPS_API_KEY=your_real_google_maps_api_key

# Supabase
VITE_SUPABASE_URL=https://vqhjkgkmxnxtcxcdremp.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key   # (from Supabase project settings)

# App Configuration
VITE_APP_NAME=Qwiky
VITE_APP_VERSION=1.0.0
VITE_DEFAULT_LOCALE=en
VITE_SUPPORTED_LOCALES=en,hi

# API Configuration
VITE_API_TIMEOUT=10000
VITE_RETRY_ATTEMPTS=3

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_PUSH_NOTIFICATIONS=false
VITE_DEBUG_MODE=true
