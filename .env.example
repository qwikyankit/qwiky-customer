# Environment Configuration
NODE_ENV=development

# Backend API URL
VITE_API_BASE_URL=http://localhost:3001

# Payment Configuration - Cashfree Only
VITE_PAYMENT_MODE=cashfree-test
# Options: cashfree-test, cashfree-live

# Frontend Environment Variables
VITE_API_BASE_URL=http://localhost:3001/api
VITE_CASHFREE_ENV=TEST
# Options: TEST, PRODUCTION

# Backend Environment Variables (for API routes)
CASHFREE_APP_ID=your_cashfree_test_app_id
CASHFREE_SECRET_KEY=your_cashfree_test_secret_key
CASHFREE_APP_ID_PROD=your_cashfree_prod_app_id
CASHFREE_SECRET_KEY_PROD=your_cashfree_prod_secret_key
CASHFREE_ENV=TEST
# Options: TEST, PRODUCTION

# Google Maps
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Supabase
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# App Configuration
VITE_APP_NAME=Qwiky
VITE_APP_VERSION=1.0.0
VITE_DEFAULT_LOCALE=en
VITE_SUPPORTED_LOCALES=en,hi

# API Configuration
VITE_API_TIMEOUT=10000
VITE_RETRY_ATTEMPTS=3

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_PUSH_NOTIFICATIONS=false
VITE_DEBUG_MODE=true