{
  "expo": {
    "name": "Qwiky Admin",
    "slug": "qwiky-admin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "qwiky-admin",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.qwiky.admin"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#4e2780"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.qwiky.admin"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "name": "Qwiky Admin",
      "shortName": "QAdmin",
      "description": "Internal Booking Management System",
      "themeColor": "#4e2780",
      "backgroundColor": "#ffffff",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#4e2780"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
